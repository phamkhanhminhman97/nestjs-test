{
  "name": "base",
  "version": "1.0.0",
  "description": "base",
  "scripts": {
    "prebuild:prod": "rimraf dist",
    "build:prod": "tsc -p tsconfig.json && cp -R src/i18n dist/src/i18n",
    "start:prod": "node dist/src/index.js",
    "format": "prettier --write \"**/*.ts\"",
    "dev": "rimraf dist && cross-env ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json && cp -R src/i18n dist/src/i18n",
    "typeorm": "npx typeorm",
    "lint": "prettier --write \"**/*.ts\" && eslint src/api --ext .ts --fix",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n"
  },
  "dependencies": {
    "@nestjs/bull": "11.0.2",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "11.0.10",
    "@nestjs/schedule": "5.0.1",
    "@nestjs/swagger": "11.0.4",
    "@nestjs/typeorm": "9.0.1",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/cron": "^1.7.3",
    "axios": "1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "nestjs-i18n": "10.3.7",
    "pg": "^8.7.1",
    "rxjs": "7.8.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.45",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.1",
    "@types/node": "22.13.5",
    "@types/supertest": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.31.2",
    "@typescript-eslint/parser": "^8.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "26.6.0",
    "lint-staged": "v14",
    "prettier": "3.5.2",
    "supertest": "^3.0.0",
    "ts-jest": "26.5.1",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.11.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "5.7.3"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  }
}
